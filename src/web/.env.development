REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=5000
REACT_APP_API_RETRY_ATTEMPTS=3

# Authentication Configuration
REACT_APP_MSAL_CLIENT_ID=your-dev-client-id
REACT_APP_MSAL_AUTHORITY=https://login.microsoftonline.com/your-tenant-id
REACT_APP_MSAL_REDIRECT_URI=https://localhost:3000
REACT_APP_MSAL_SCOPES=api://localhost/access

# Excel Add-in Configuration
REACT_APP_VERSION=0.1.0
REACT_APP_EXCEL_TASKPANE_WIDTH=350
REACT_APP_EXCEL_TASKPANE_HEIGHT=600

# OpenAI Configuration
REACT_APP_OPENAI_API_KEY=your-openai-api-key

# Development Features
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_MOCK_API=true
REACT_APP_CACHE_DURATION=300

# SSL Configuration for Development
SSL_CRT_FILE=./certificates/localhost.crt
SSL_KEY_FILE=./certificates/localhost.key

# Development Server Configuration
PORT=3000
NODE_ENV=development

# Security Configuration
REACT_APP_TOKEN_ENCRYPTION_KEY=your-dev-encryption-key
REACT_APP_CONFIG_ENCRYPTION_KEY=your-dev-config-key

# Performance Configuration
REACT_APP_MAX_FORMULA_LENGTH=8192
REACT_APP_MAX_RANGE_SIZE=10000
REACT_APP_SYNC_INTERVAL=2000
REACT_APP_SUGGESTION_TIMEOUT=5000
REACT_APP_MAX_CONCURRENT_REQUESTS=5

# Rate Limiting
REACT_APP_RATE_LIMIT_FORMULA=100
REACT_APP_RATE_LIMIT_DATA=20
REACT_APP_RATE_LIMIT_VERSION=200

# Error Reporting
REACT_APP_ERROR_REPORTING_LEVEL=debug
REACT_APP_ERROR_REPORTING_ENABLED=true

# Feature Flags
REACT_APP_FEATURE_VERSION_CONTROL=true
REACT_APP_FEATURE_DATA_CLEANING=true
REACT_APP_FEATURE_AI_SUGGESTIONS=true

# Monitoring and Telemetry
REACT_APP_ENABLE_TELEMETRY=true
REACT_APP_PERFORMANCE_MONITORING=true