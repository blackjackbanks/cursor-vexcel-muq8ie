# Application Version
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_BASE_URL=https://api.excel-addin.com
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=2000

# Authentication Configuration
REACT_APP_AUTH_TENANT_ID=production-tenant-id
REACT_APP_AUTH_CLIENT_ID=production-client-id
REACT_APP_AUTH_REDIRECT_URI=https://excel-addin.com
REACT_APP_AUTH_SCOPES=openid profile email

# OpenAI Configuration
REACT_APP_OPENAI_API_KEY=production-openai-key

# Monitoring and Logging
REACT_APP_ENABLE_MONITORING=true
REACT_APP_LOG_LEVEL=error
REACT_APP_ERROR_REPORTING_ENDPOINT=https://monitoring.excel-addin.com/errors
REACT_APP_PERFORMANCE_TRACKING_ENABLED=true

# Cache Configuration
REACT_APP_CACHE_DURATION=3600

# Retry Configuration
REACT_APP_MAX_RETRIES=3
REACT_APP_RETRY_DELAY=1000

# UI Configuration
REACT_APP_TASK_PANE_WIDTH=350
REACT_APP_TASK_PANE_MIN_WIDTH=50

# Rate Limiting
REACT_APP_RATE_LIMIT_REQUESTS=100
REACT_APP_RATE_LIMIT_INTERVAL=60000