# Application Version
REACT_APP_VERSION=0.1.0

# API Configuration
REACT_APP_API_BASE_URL=https://api-staging.excel-addin.com
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=5000
REACT_APP_API_REGIONS=us-east,eu-west,asia-east

# Authentication Configuration
REACT_APP_AUTH_TENANT_ID=staging-tenant-id
REACT_APP_AUTH_CLIENT_ID=staging-client-id
REACT_APP_AUTH_REDIRECT_URI=https://staging.excel-addin.com/auth/callback
REACT_APP_AUTH_SCOPES=openid profile email offline_access api://excel-addin-staging/user_impersonation

# AI Service Configuration
REACT_APP_OPENAI_API_KEY=sk-staging-key

# Environment Settings
REACT_APP_ENVIRONMENT=staging
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_DEBUG=true

# Monitoring and Telemetry
REACT_APP_ENABLE_TELEMETRY=true
REACT_APP_APPLICATION_INSIGHTS_KEY=staging-insights-key
REACT_APP_ERROR_MONITORING=true
REACT_APP_PERFORMANCE_MONITORING=true

# Cache Configuration
REACT_APP_CACHE_DURATION=300
REACT_APP_MAX_RETRIES=3

# Feature Flags
REACT_APP_FEATURE_FLAGS=ai-suggestions:true,data-cleaning:true,version-control:true